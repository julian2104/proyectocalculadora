    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
     calcu2.num2=Float.parseFloat(Textoprincipal.getText());
     calcu2.porceniva=Float.parseFloat(Textoprincipal.getText());
     b=calcu2.num2;
    switch (simbolo) {
        case "+":
            calcu2.suma();
              Textoprincipal.setText(String.valueOf(calcu2.result));
            break;
        case "-":
            calcu2.resta();
              Textoprincipal.setText(String.valueOf(calcu2.result));
            break;
        case "*":
            calcu2.mult();
              Textoprincipal.setText(String.valueOf(calcu2.result));
            break;
        case "/":
            calcu2.div();
            if (calcu2.num2 != 0) {
                    calcu2.div();
                      Textoprincipal.setText(String.valueOf(calcu2.result));
                } else {
                    Textoprincipal.setText("Error: DivisiÃ³n por cero");
                    return;
                }
            break;
        case "sen":
            calcu2.sen(b);
            Textoprincipal.setText(String.valueOf(calcu2.result));
            break;
        case "cos":
            calcu2.cos(b);
            Textoprincipal.setText(String.valueOf(calcu2.result));
            break;
        case "tan":
            calcu2.tan(b);
            Textoprincipal.setText(String.valueOf(calcu2.result));
            break;
        case "raiz":
            calcu2.raiz(a,b);
            if (calcu2.num2 > 0){
                calcu2.raiz(a,b);
            Textoprincipal.setText(String.valueOf(calcu2.result));
            }else {
                Textoprincipal.setText("Error: numero imaginario");
                return;
            }
            break;
        case "Potencia":
            calcu2.pot(a,b);
            Textoprincipal.setText(String.valueOf(calcu2.result));
            break;
        case "iva":
            calcu2.iva();
            Textoprincipal.setText(String.valueOf(calcu2.result));
            break;
            
    }
    }                                        

